public class StatLowerMove extends Move {
	private float accuracy;
	private int stat;
	private float amountLower;
	public StatLowerMove(String description, String name, int pp, int stat, float amountLower) {
		this.description = description;
		this.name = name;
		this.type = type;
		this.pp = pp;
		this.accuracy = 1;
		this.type = type;
		this.amountLower = amountLower;
	}
}
